import tkinter as tk
from tkinter import filedialog, Text
import webbrowser
import os

def run_social():
    webbrowser.open(url1,number)
    webbrowser.open(url2,number)
    webbrowser.open(url3,number)

def run_browser():
    os.startfile('"C:\Program Files (x86)\Google\Chrome\Application\chrome.exe"')



root =tk.Tk()
root.title("Open Taps")
number = 3

url1 ="https://www.pexels.com"

url2 ="https://www.instagram.com"

url3 ="https://www.facebook.com"

button1= tk.Button(root, text="Open Browser", command=run_browser)
button1.pack()

button2= tk.Button(root, text="Open social media", command=run_social)
button2.pack()



root.mainloop()
